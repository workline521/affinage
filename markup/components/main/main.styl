.main
    display flex
    flex-wrap wrap
    justify-content center
    align-items center
    width 100%
    height 100%
    position relative
    @media (max-width: 1450px)
        padding-bottom 27px
    @media (max-width: 1400px)
        padding-bottom 0
    &__background-left
        position absolute
        content ''
        width 50%
        height 100%
        left 0
        top 0
        background #fefefe
        z-index -1
        &:after
            position absolute
            content ''
            width 83.33%
            height 68.87%
            right 0
            bottom 0
            box-shadow 0 0 99px rgba(0, 0, 0, 0.1)
            opacity 0.65
        @media (max-width: 767px)
            display none
    &__background-right
        position absolute
        content ''
        width 50%
        height 100%
        right 0
        top 0
        box-shadow 0 0 99px rgba(0, 0, 0, 0.1)
        background-color #ffffff
        z-index 0
        @media (max-width: 767px)
            display none
    &__vertical-txt
        position absolute
        color rgba(0,0,0, 0.4)
        font-weight 300
        font-style italic
        font-size 12px
        line-height 14.4px
        letter-spacing 0.44px
        writing-mode vertical-lr
        top 50%
        transform translateY(-50%) rotate(180deg)
        left 22px
        @media (max-width: 999px)
            left 16px
        @media (max-width: 860px)
            display none
    &__decoration
        position absolute
        width 23px
        height 23px
    &__decoration--1
        bottom 84px
        left 26.80%
        background url('data:image/png;base64,') no-repeat center
    &__decoration--2
        bottom -5px
        left 62.6%
        background url('data:image/png;base64,') no-repeat center
    &__decoration--3
        bottom 23px
        left 34.02%
        background url('data:image/png;base64,') no-repeat center
    &__decoration--4
        bottom 42px
        left 66.66%
        background url('data:image/png;base64,') no-repeat center
    &__decoration--5
        top -38px
        right 24.84%
        background url('data:image/png;base64,') no-repeat center
        width 59px
        height 58px
        background-size cover
        @media (max-width: 999px)
            top -80px
        @media (max-width: 767px)
            top 42%
            width 45px
            height 45px
            background-size cover
            z-index 9
            left 21.875%
    &__decoration--6
        top 123px
        right 18.59%
        background url('data:image/png;base64,') no-repeat center
        width 54px
        height 60px
        background-size cover
        z-index 7
        @media (max-width: 1400px)
            top 90px
        @media (max-width: 767px)
            top 39%
            width 42px
            height 46px
            background-size cover
            z-index 6
            left 46.875%
    &__decoration--7
        top 45px
        right 12.10%
        background url('data:image/png;base64,') no-repeat center
        width 43px
        height 58px
        background-size cover
        @media (max-width: 1400px)
            top 25px
        @media (max-width: 999px)
            top 0
        @media (max-width: 767px)
            top 25.25%
            width 33px
            height 44px
            background-size cover
            z-index 9
            left 69.37%
    &__decoration--8
        display none
        @media (max-width: 767px)
            display block
            bottom 30px
            left 14.06%
            background url('data:image/png;base64,') no-repeat center
            z-index 8
    &__decoration--9
        display none
        @media (max-width: 767px)
            display block
            bottom 56%
            right 8.75%
            background url('data:image/png;base64,') no-repeat center
            z-index 8
    &__letter
        position absolute
        font-size 25px
        font-weight 700
        line-height 25px
        text-transform uppercase
    &__letter--1
        top 31px
        left 35px
        font-size 33px
        line-height 33px
        z-index 8
        @media (max-width: 999px)
            top 12px
            left 20px
    &__letter--2
        bottom 31px
        left 37.08%
    &__letter--3
        bottom 67px
        left 46%
    &__letter--4
        bottom 22px
        left 55%
    &__letter--5
        bottom 50px
        left 63.1%
    &__letter--6
        bottom 11px
        left 72.5%
    &__letter--7
        bottom 53px
        left 29.16%
    &__letter--8
        bottom 45px
        left 43.15%
        &:after
            position absolute
            content ''
            top 3px
            left -5px
            width 21px
            height 21px
            background url('data:image/png;base64,') no-repeat center
    &__letter--9
        bottom 18px
        left 58.75%
    &__wrapper
        display flex
        flex-wrap wrap
        justify-content center
        align-items center
        width 1280px
        height 577px
        position relative
        z-index 7
        &:after
            position absolute
            content ''
            width 160px
            height 112px
            bottom -32px
            left 50%
            transform translateX(-50%)
            background url('data:image/png;base64,') no-repeat center
            z-index 8
            @media (max-width: 1400px)
                width 100px
                height 70px
                background-size cover
            @media (max-width: 767px)
                width 110px
                height 77px
                bottom -24px
        &:before
            position absolute
            content ''
            width 212px
            height 10px
            background #f7e611
            bottom -42px
            left 50%
            transform translateX(-50%)
            z-index 8
            @media (max-width: 1400px)
                width 160px
            @media (max-width: 767px)
                display none
        @media (max-width: 1450px)
            padding-top 20px
        @media (max-width: 1400px)
            padding-top 0
            width 900px
            height 406px
        @media (max-width: 999px)
            width 768px
            height 600px
        @media (max-width: 767px)
            width 100%
            height 100%
            background #f7e611
            justify-content flex-start
            align-items flex-start
            padding-top 85px
            overflow hidden
            position relative
    &__slider2-prev
        position absolute
        opacity 0
        z-index -1
    &__slider2-next
        position absolute
        opacity 0
        z-index -1
    &__slider1-prev
        position absolute
        border 0
        padding 0
        background transparent
        outline none
        width 34px
        height 34px
        bottom 120px
        left -44px
        svg
            width 100%
            height 100%
            fill rgba(0,0,0, 0.4)
            transition 0.3s ease-out
        &:hover svg
            fill #000
        @media (max-width: 1400px)
            bottom 42px
        @media (max-width: 767px)
            left 13px
            bottom auto
            top 50%
            transform translateY(-50%)
    &__slider1-next
        position absolute
        border 0
        padding 0
        background transparent
        outline none
        width 34px
        height 34px
        bottom 120px
        right -44px
        svg
            width 100%
            height 100%
            fill rgba(0,0,0, 0.4)
            transition 0.3s ease-out
        &:hover svg
            fill #000
        @media (max-width: 1400px)
            bottom 42px
        @media (max-width: 767px)
            right 13px
            bottom auto
            top 50%
            transform translateY(-50%)
    &__slider-nav
        position absolute
        bottom 36px
        right 15.46%
        width 138px
        height 383px
        background #f7e611
        z-index 7
        display flex
        flex-wrap wrap
        align-items flex-end
        padding-left 19px
        padding-bottom 21px
        @media (max-width: 1400px)
            height 240px
        @media (max-width: 767px)
            width 100%
            height 56vh
            background transparent
            bottom 0
            right 0
            padding 0

        &:after
            display none
            @media (max-width: 767px)
                display block
                position absolute
                content ''
                right 0
                bottom 0
                width 0
                height 0
                border-style solid
                border-width 0 0 105px 105px
                border-color transparent transparent #f7e811 transparent

    &__all-slides
        font-family 'Geometria', sans-serif
        font-size 45px
        font-weight 900
        line-height 45px
        font-style italic
        letter-spacing 4.5px
        margin-bottom 16px
        position relative
        &:after
            position absolute
            content ''
            bottom 6px
            right -18px
            width 17px
            height 3px
            background #000000
        @media (max-width: 1400px)
            font-size 36px
            line-height 39px
        @media (max-width: 767px)
            display none
    &__current-slide
        font-family 'Geometria', sans-serif
        font-size 17px
        font-weight 900
        line-height 17px
        font-style italic
        letter-spacing 1.7px
        @media (max-width: 767px)
            position absolute
            width 25px
            text-align center
            bottom 19px
            right 17px
            font-size 20px
            font-weight 900
            line-height 24px
            letter-spacing 2px
            z-index 2
    &__title
        position relative
        z-index 8
        @media (max-width: 999px)
            width 100%
        @media (max-width: 767px)
            height 1px
            position absolute
            left 0
            bottom 56%
        h1
            color #ffffff
            font-size 140px
            font-weight 400
            line-height 140px
            text-transform uppercase
            letter-spacing 50.4px
            margin-left 53px
            position relative
            z-index 2
            @media (max-width: 1400px)
                font-size 78px
                line-height 78px
                margin-left 40px
            @media (max-width: 999px)
                display none
    &__slogan
        position absolute
        color rgba(0,0,0, 0.7)
        font-size 9px
        font-style italic
        line-height 9px
        writing-mode vertical-lr
        bottom -65px
        transform rotate(180deg)
        left 152px
        @media (max-width: 1400px)
            left 105px
        @media (max-width: 999px)
            left auto
            right 580px
            height 100px
            text-align center
            bottom -120px
        @media (max-width: 767px)
            left 21px
            width 9px
        @media (max-width: 425px)
            bottom -44px
    &__trapezoid
        display block
        position absolute
        left 178px
        top -62px
        width 177px
        height 282px
        border-top 1px solid #000
        border-left 1px solid #000
        &:after
            position absolute
            content ''
            width 141px
            height 1px
            background #000
            left 0
            bottom 0
            @media (max-width: 1400px)
                width 98px
        &:before
            position absolute
            content ''
            width 1px
            height 283px
            background #000
            right 0
            top 0
            transform-origin 100% 0
            transform rotate(7deg)
            @media (max-width: 1400px)
                height 230px
        @media (max-width: 1400px)
            left 135px
            width 127px
            height 230px
        @media (max-width: 999px)
            display none
    &__slider2
        position absolute !important
        top 0
        left 0
        width 800px
        z-index 5
        background rgba(247, 230, 17, 0.8)
        .slick-active
            img
                transform scale(1, 1)
        @media (max-width: 1400px)
            width 563px
        @media (max-width: 767px)
            width calc(100% - 84px)
            height calc(44% - 85px)
            top auto
        .slick-list
            @media (max-width: 767px)
                height 100%
        .slick-track
            @media (max-width: 767px)
                height 100%
        &:after
            display none
            @media (max-width: 767px)
                display block
                position absolute
                content ''
                width 1px
                height 85px
                top -85px
                left 70%
                background #000
                z-index -1
        &:before
            display none
            @media (max-width: 767px)
                display block
                position absolute
                content ''
                width 100%
                height 1px
                top 50px
                right -70%
                background #000
                z-index -1
                transform rotate(8deg)
            @media (max-width: 425px)
                top 27px
    &__slider1
        position absolute !important
        right 0
        bottom 0
        width 860px
        z-index 6
        background rgba(247, 230, 17, 0.8)
        .slick-active
            img
                transform scale(1, 1)
        @media (max-width: 1400px)
            width 605px
        @media (max-width: 767px)
            width 100%
            height 56%
        .slick-list
            @media (max-width: 767px)
                height 100%
        .slick-track
            @media (max-width: 767px)
                height 100%
    &__slide
        height 490px !important
        position relative
        @media (max-width: 1400px)
            height 345px !important
        @media (max-width: 767px)
            height 100% !important
        &:after
            position absolute
            content ''
            top 0
            left 0
            width 100%
            height 100%
            background rgba(247, 230, 17, 0.8)
            z-index 0
            @media (max-width: 767px)
                background rgba(247, 230, 17, 0.6)
        img
            display block
            position absolute
            width 100%
            height 100%
            top 0
            left 0
            bottom 0
            right 0
            will-change transform
            transition all 500ms ease-in-out
            transform scale(1.25, 1.25)
    &__slide2
        height 525px !important
        position relative
        @media (max-width: 1400px)
            height 369px !important
        @media (max-width: 767px)
            height 100% !important
        &:after
            position absolute
            content ''
            top 0
            left 0
            width 100%
            height 100%
            background rgba(247, 230, 17, 0.8)
            z-index 0
        img
            display block
            position absolute
            width 100%
            height 100%
            top 0
            left 0
            bottom 0
            right 0
            will-change transform
            transition all 500ms ease-in-out
            transform scale(1.25, 1.25)
    &__slide-caption
        display flex
        flex-wrap wrap
        justify-content center
        align-items center
        width 102px
        height 102px
        border 1px solid #000
        border-radius 50%
        position relative
        z-index 1
        margin-left 84px
        margin-top 31px
        color #ffffff
        font-family 'Geometria', sans-serif
        font-size 17px
        font-weight 400
        line-height 20px
        text-transform uppercase
        letter-spacing 2.14px
        padding-left 23px
        padding-right 21px
        @media (max-width: 1400px)
            margin-top 20px
            margin-left 60px
            width 70px
            height 70px
            font-size 14px
        @media (max-width: 767px)
            display none
    &__slide-txt
        display block
        position absolute
        z-index 2
        top 84px
        left 144px
        width 200px
        p
            display block
            color #000000
            font-family 'Geometria', sans-serif
            font-size 14px
            font-weight 300
            line-height 14px
            &:first-child
                margin-bottom 66px
        @media (max-width: 1400px)
            top 70px
            left 70px
        @media (max-width: 767px)
            display none
    &__slide-integer
        font-family 'Geometria', sans-serif
        position absolute
        color rgba(0,0,0, 0.5)
        font-size 28px
        font-weight 900
        line-height 28px
        letter-spacing 2.8px
        writing-mode vertical-lr
        transform rotate(180deg)
        right 36.5%
        top 28px
        z-index 2
        @media (max-width: 1400px)
            top 15px
        @media (max-width: 767px)
            display none
